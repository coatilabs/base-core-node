{
  "name": "coatilabs-node-core",
  "version": "1.0.0",
  "description": "Backend core",
  "private": true,
  "main": "dist/main.js",
  "scripts": {
    "build": "gulp",
    "start": "node --require source-map-support/register dist/main.js",
    "db:migrate": "sequelize db:migrate",
    "db:reset": "sequelize db:drop && sequelize db:create && sequelize db:migrate",
    "db:seed": "sequelize db:seed:all",
    "pretest": "rm -rf ./db.sqlite && NODE_ENV=test sequelize db:migrate",
    "test": "mocha -r ts-node/register tests/**/*.test.ts",
    "testWithCoverage": "nyc -r text -e .ts -x \"tests/*.test.ts\" mocha -r ts-node/register tests/**/*.test.ts"
  },
  "author": "Coati Labs <hola@coatilabs.com>",
